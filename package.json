    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@worktile/pkg-manager": "0.0.5",
    "codelyzer": "^6.0.0",
    "coveralls": "^3.0.9",
    "husky": "^6.0.0",
